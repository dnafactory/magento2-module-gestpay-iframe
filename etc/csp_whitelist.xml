<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="bancasella-iframe" type="host">*.sella.it</value>
                <value id="gestpay-iframe" type="host">*.gestpay.net</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="bancasella-iframe" type="host">*.sella.it</value>
                <value id="gestpay-iframe" type="host">*.gestpay.net</value>
            </values>
        </policy>
        <policy id="frame-ancestors">
            <values>
                <value id="bancasella-iframe" type="host">*.sella.it</value>
                <value id="gestpay-iframe" type="host">*.gestpay.net</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
